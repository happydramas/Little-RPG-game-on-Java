package com.company;

public class FightGoblin {
    static int mc2, goblin;

    int goblinDamage;

    public int getMc2() {
        return mc2;
    }
    public void setMc(int mc2) {
        this.mc2 = mc2;
    }

    public int getGoblin() { return goblin; }
    public void setGoblin(int goblin) {
        this.goblin = goblin;
    }

    public int getGoblinDamage() {
        return goblinDamage;
    }
    public void setGoblinDamage(int goblinDamage) {
        this.goblinDamage = goblinDamage;
    }

    public FightGoblin(int mc2, int goblin, int goblinDamage) {
        setMc(mc2);
        setGoblin(goblin);
        setGoblinDamage(goblinDamage);
    }
}
